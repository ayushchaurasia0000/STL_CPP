#include<iostream>
#include<array>

using namespace std;
int main()
{
    int i;
    std::array<int,6>a={1,2,3,4,5,6};
    std::array <int,6>b={9,8,7,6,5,4};
    a.swap(b);
    //a.fill(10);
    for( i=0;i <=6;i++)
    std::cout <<"value of a"<<endl<<a[i]<<endl;
    for( i=0;i <=6;i++)
    std::cout <<"value of b"<<endl<<b[i]<<endl;
    
    return 0;
}
