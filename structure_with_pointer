#include<bits/stdc++.h>

using namespace std;
struct ayush
{
    int  b;
};
int main()
{
    struct ayush a;
    struct ayush *p=&a;
    int q=(*p).b=20; //  this is also same to give the value
    int w =p->b=40;    // when you use pointer in structure or class 
    std::cout <<q<<endl<<w;
    return 0;
}
